[32m+[m[32m// Intersection Observer Hook[m
[32m+[m[32mconst useInView = (threshold = 0.2) => {[m
[32m+[m[32m  const [isInView, setIsInView] = useState(false);[m
[32m+[m[32m  const ref = useRef(null);[m
[32m+[m[41m  [m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    const observer = new IntersectionObserver([m
[32m+[m[32m      ([entry]) => {[m
[32m+[m[32m        if (entry.isIntersecting) {[m
[32m+[m[32m          setIsInView(true);[m
[32m+[m[32m        }[m
[32m+[m[32m      },[m
[32m+[m[32m      { threshold, rootMargin: '-50px' }[m
[32m+[m[32m    );[m
[32m+[m[41m    [m
[32m+[m[32m    if (ref.current) observer.observe(ref.current);[m
[32m+[m[32m    return () => observer.disconnect();[m
[32m+[m[32m  }, [threshold]);[m
[32m+[m[41m  [m
[32m+[m[32m  return [ref, isInView];[m
[32m+[m[32m};[m
