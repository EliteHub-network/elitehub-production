[32m+[m[32m// Counter Animation Hook[m
[32m+[m[32mconst useCountUp = (end, duration = 2000, startTrigger = false) => {[m
[32m+[m[32m  const [count, setCount] = useState(0);[m
[32m+[m[41m  [m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    if (!startTrigger) return;[m
[32m+[m[41m    [m
[32m+[m[32m    let startTimestamp = null;[m
[32m+[m[32m    const step = (timestamp) => {[m
[32m+[m[32m      if (!startTimestamp) startTimestamp = timestamp;[m
[32m+[m[32m      const progress = Math.min((timestamp - startTimestamp) / duration, 1);[m
[32m+[m[41m      [m
[32m+[m[32m      const easedProgress = progress < 0.5[m[41m [m
[32m+[m[32m        ? 2 * progress * progress[m[41m [m
[32m+[m[32m        : 1 - Math.pow(-2 * progress + 2, 3) / 2;[m
[32m+[m[41m      [m
[32m+[m[32m      setCount(Math.floor(easedProgress * end));[m
[32m+[m[41m      [m
[32m+[m[32m      if (progress < 1) {[m
[32m+[m[32m        window.requestAnimationFrame(step);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[41m    [m
[32m+[m[32m    window.requestAnimationFrame(step);[m
[32m+[m[32m  }, [end, duration, startTrigger]);[m
[32m+[m[41m  [m
[32m+[m[32m  return count;[m
[32m+[m[32m};[m
